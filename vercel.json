{
    "cleanUrls": true,
    "headers": [
        {
            "source": "/songs/(.*)",
            "headers": [
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                }
            ]
        }
    ],
    "rewrites": [
        {
            "source": "/songs/:match*",
            "destination": "/songs/:match*"
        }
    ]
}